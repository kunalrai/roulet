                 }

                if (!$.Game.busy) {

                    $.Game.busy = true;

                    $.ajax({
                    type: "POST",
                        url: "/games/find/",
                        data: JSON.stringify(data),
                        success: function (response) {

                            $.Game.busy = false;

                        if (response.must_bet) {

