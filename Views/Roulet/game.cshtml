                    for (var i = 0; i < hidden_bets.length; i++) {

                        if (id == hidden_bets[i]) {
                           
                            return true;
                        }

                    }

                }

                return false;
            },
            RemoveBet: function (id) {

                var hidden_bets = $.Game.bets;

                if (hidden_bets.length > 0) {

                    for (var i = 0; i < hidden_bets.length; i++) {

                        if (id == hidden_bets[i]) {

                            hidden_bets.splice(i, 1);
                        }

                    }

                }

                $.Game.bets = hidden_bets;

             }

